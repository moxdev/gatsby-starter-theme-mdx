{
  "name": "gatsby-starter-theme-prettier-eslint",
  "description": "Gatsby starter theme with Prettier & Eslint configurations.",
  "version": "2.0.0",
  "author": "Shane Biggs",
  "dependencies": {
    "gatsby": "^2.1.20",
    "gatsby-image": "^2.0.31",
    "gatsby-plugin-manifest": "^2.0.21",
    "gatsby-plugin-offline": "^2.0.24",
    "gatsby-plugin-react-helmet": "^3.0.7",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sharp": "^2.0.24",
    "gatsby-plugin-styled-components": "^3.0.6",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-sharp": "^2.1.15",
    "node-sass": "^4.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "watch-config": "gatsby develop && watch-run -p gatsby-config.js 'kill-port --port 8000 && gatsby develop'",
    "dev": "gatsby develop",
    "format": "prettier --write '**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
